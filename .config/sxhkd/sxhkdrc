super + shift + m
   ~/Scripts/mute.sh; ~/Scripts/refresh.sh

super + control + m
   ~/Scripts/mic.sh; ~/Scripts/refresh.sh

super + shift + u
    amixer set Master 5%+ &>/dev/null; ~/Scripts/refresh.sh

super + shift + d
    amixer set Master 5%- &>/dev/null; ~/Scripts/refresh.sh

super + control + u
    amixer set Capture 5%+ &>/dev/null; ~/Scripts/refresh.sh

super + control + d
    amixer set Capture 5%- &>/dev/null; ~/Scripts/refresh.sh

super + shift + s
    flameshot gui
